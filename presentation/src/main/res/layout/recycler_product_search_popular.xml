<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <com.google.android.material.card.MaterialCardView
        android:id="@+id/cardview_search_popular"
        style="@style/CardViewProductSearch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingVertical="8dp"
            android:paddingHorizontal="10dp">
            <TextView
                android:id="@+id/textview_search_popular_rank"
                style="@style/TextViewProductSearch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/bidderbidder_primary"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                tools:text="01" />
            <TextView
                android:id="@+id/textview_search_popular"
                style="@style/TextViewProductSearch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                app:layout_constraintTop_toTopOf="@id/textview_search_popular_rank"
                app:layout_constraintBottom_toBottomOf="@id/textview_search_popular_rank"
                app:layout_constraintStart_toEndOf="@id/textview_search_popular_rank"
                tools:text="대충 검색어" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>
</layout>
